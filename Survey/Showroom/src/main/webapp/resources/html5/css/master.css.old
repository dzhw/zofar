	background-color: #f9f9f9;
	border-color: #dcdcdc;
}

.noUi-target {
	background: #dfdfdf;
	border: 1px solid #ccc;
}

.noUi-handle {
	visibility: hidden;
	background: #006ab2;
	box-shadow: none;
	border-color: #005b99;
}

.noUi-handle.focus {
	border-color: orange;
}

.noUi-handle.noUi-active, .noUi-state-tap .noUi-handle {
	background-color: green;
}

.noUi-tooltip {
	font-size: 12px;
	font-size: 1.2rem;
