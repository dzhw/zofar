.noui-slider {
	height: 50vh;
	width: 100%;
	margin: 30px 15px;
	margin: 3rem 1.5rem;
	transition: height 1000000s ease;
}

.noui-slider .noUi-value {
	color: #666;
}

.noui-slider .noUi-marker {
	width: 16px;
	height: 16px;
	left: 50%;
	margin-top: -9px;
	margin-left: -8px;
	border-radius: 50%;
	background: #fff;
	border: 1px solid #ccc;
}

.noui-slider .noUi-marker.noUi-marker-large {
	width: 26px;
	height: 26px;
	margin-top: -13px;
	margin-left: -13px;
}

.noui-slider:focus {
	outline: 0;
	background: #ccc;
}

.noui-slider[disabled=true] {
	background-color: #f9f9f9;
	border-color: #dcdcdc;
}

.noui-slider[disabled=true] .noUi-handle {
	background-color: #f9f9f9;
