        <zofar:variable name="jsCheck" type="boolean"/>
        <zofar:variable name="url" type="string"/>
<!--         	!!! activate for multi langauage survey -->
<!--         <zofar:variable name="language" type="string"/> -->
	</zofar:variables>
    <zofar:page uid="index">
        <zofar:header>
            <zofar:title uid="t1">Das ist eine Zofar Befragung.</zofar:title>
            <zofar:text uid="t2">Hier haben Sie die Möglichkeit eine Befragung zu erstellen.</zofar:text>
        </zofar:header>
        <zofar:body uid="body">
		</zofar:body>
		<zofar:triggers>
			<zofar:variable variable="url" value="zofar.baseUrl()"></zofar:variable>
			<!-- <zofar:variable variable="uas" value="zofar.userAgent()"></zofar:variable> -->
			
			<zofar:jsCheck variable="jsCheck" xvar="width" yvar="height">
			</zofar:jsCheck>
			<zofar:variable variable="isMobile" value="zofar.isMobile()"></zofar:variable>
		</zofar:triggers>
		<zofar:transitions>
			<zofar:transition target="offer" condition="jsCheck.value and isMobile.value and width.value lt 400"/>
			<zofar:transition target="end" />
		</zofar:transitions>
    </zofar:page>
    <zofar:page uid="offer">
		<zofar:header>
			<zofar:title uid="t" block="true">
				Um ein optimales Befragungserlebnis zu erhalten, sollten Sie für die Beantwortung einiger Fragen das Display drehen.
			</zofar:title>
		</zofar:header>
		<zofar:body uid="b"></zofar:body>
		<zofar:transitions>
			<zofar:transition target="end"/>
		</zofar:transitions>
	</zofar:page>
    <zofar:page uid="end">
        <zofar:header>
            <zofar:title uid="t1">Vielen Dank!</zofar:title>
        </zofar:header>
    </zofar:page>
</zofar:questionnaire>
